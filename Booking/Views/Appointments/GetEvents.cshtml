
@{
    ViewData["Title"] = "GetEvents";
}
<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.10.2/fullcalendar.min.css" integrity="sha256-jIeXq6gB00N98uOKw7M9n7dH3y2MBFvVILEndI7K+Cw=" crossorigin="anonymous" />

    <!-- FullCalendar JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js" integrity="sha256-l2b/Xe8YQEKc6zU2FC0Bz5nU9DNQNs9zgWmBPrNo3Q0=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.10.2/fullcalendar.min.js" integrity="sha256-G5aSdFLYoPfLrO1tXzzxzk/p6/VF2z5elBtQ/7/HaMY=" crossorigin="anonymous"></script>
</head>
<body>
    <div id="calendar"></div>

    <script>
        $(document).ready(function () {
            $.ajax({
                url: '@Url.Action("GetEvents", "Appointments")',
                type: 'GET',
                success: function (events) {
                    $('#calendar').fullCalendar({
                        header: {
                            left: 'prev,next today',
                            center: 'title',
                            right: 'month,agendaWeek,agendaDay'
                        },
                        events: events
                    });
                },
                error: function (error) {
                    console.error('Error fetching events:', error);
                }
            });
        });
    </script>



</body>


